{
  "version": 2,
  "builds": [
    {
      "src": "restaurant_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "restaurant_app/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "restaurant_app.settings",
    "SECRET_KEY": "@mysecretkey",
    "DATABASE_URL": "@mydatabaseurl",
    "GOOGLE_API_KEY": "@mygoogleapikey"
  },
  "buildCommand": "python manage.py collectstatic --noinput"
}
